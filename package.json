{
  "name": "cloudflare-inquiry-web-form",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:pages": "wrangler pages dev dist --port 8788",
    "build": "tsc -b && vite build",
    "deploy:pages": "wrangler pages deploy dist",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md,html,htm}'",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html,htm}\"",
    "typecheck": "tsc -b",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "hono": "^4.9.0",
    "mimetext": "^3.0.27",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vitest": "^3.2.4",
    "wrangler": "^4.58.0"
  },
  "pnpm": {
    "overrides": {
      "hono@<4.11.4": ">=4.11.4",
      "undici@>=7.0.0 <7.18.2": ">=7.18.2"
    }
  }
}
